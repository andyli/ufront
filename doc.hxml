-main ImportAll
-lib mockatoo
-lib utest
--no-output
--each

# Neko
-lib ufront
-cp doc/src
-D server
-neko doc/neko.n
-xml doc/neko.xml

# PHP
--next
-D server
-lib ufront
-cp doc/src
-php doc/php/
-xml doc/php.xml

# JS (Client)
--next
-lib ufront-client
-cp doc/src
-js doc/client.js
-xml doc/js-client.xml

# NodeJS
--next
-lib ufront
-lib hxnodejs
-lib express
-cp doc/src
--macro allowPackage("sys")
-js doc/nodejs.js
-xml doc/js-node.xml
